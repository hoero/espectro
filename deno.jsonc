{
  "compilerOptions": {
    "allowJs": true,
    "lib": [
      "deno.window",
      "dom"
    ],
    "strict": true
  },
  "importMap": "import_map.json",
  "tasks": {
    "dev": "deno run --watch main.ts",
    "esm:add": "deno run -A https://esm.sh/v106 add",
    "esm:update": "deno run -A https://esm.sh/v106 update",
    "esm:remove": "deno run -A https://esm.sh/v106 remove"
  }
}